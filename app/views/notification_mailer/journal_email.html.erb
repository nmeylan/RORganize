<% journal = @notification.trigger.decorate %>
<% model = @notification.model %>
<% if journal.action_type.eql?(Journal::ACTION_UPDATE) %>
    <%= render partial: 'journal_update', locals: {journal: journal, model: model} %>
<% elsif journal.action_type.eql?(Journal::ACTION_CREATE) %>
    <%= render partial: 'journal_create', locals: {journal: journal, model: model} %>

<% end %>

