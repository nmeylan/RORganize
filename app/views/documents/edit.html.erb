<%= contextual t(:link_edit_document) %>

<%= form_for(@document_decorator, :url => document_path, :method => :put, :html => {:class => 'form', :multipart => true}) do |f| %>
    <%= render :partial => 'documents/form', :locals => {:f => f, document_decorator: @document_decorator, project: @project} %>
    <%= submit_tag t(:button_save) %>
<% end %>
