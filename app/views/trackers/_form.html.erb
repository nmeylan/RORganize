<%= rorganize_form_for tracker, wrapper: :horizontal_form, html: {class: 'form-horizontal', multipart: true} do |f| %>
    <%= raw error_messages(tracker.errors.full_messages) %>

    <%= f.input :name, input_html: {maxlength: 50} %>

    <%= f.button :submit %>
<% end %>