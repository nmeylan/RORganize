<%= contextual t(:link_new_issue) %>
<%= form_for @issue_decorator, :url => {:controller => 'issues'}, :html => {:class => 'form', :multipart => true} do |f| %>
    <%= render :partial => 'issues/form', :locals => {:f => f, :form_content => form_content, issue_decorator: @issue_decorator, project: @project} %>
    <%= submit_tag t(:button_create) %>
<% end %>