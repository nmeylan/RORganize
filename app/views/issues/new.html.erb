<div class="contextual">
  <h1><%= t(:link_new_issue) %></h1>
</div>
<%= form_for @issue, :url => {:controller => 'issues'}, :html => {:class => 'form',:multipart => true} do |f|%>
  <%= render :partial => 'issues/form', :locals => {:f => f, :form_content => form_content} %>
  <%= submit_tag t(:button_create) %>
<% end %>