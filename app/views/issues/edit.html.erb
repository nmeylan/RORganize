<div class="contextual">
  <h1><%= t(:label_edit_issue) %></h1>
</div>
<%= form_for(@issue, :url => issue_path, :method => :put, :html => {:class => 'form',:multipart => true}) do |f|%>
  <%= render :partial => 'issues/form', :locals => {:f => f, :form_content => form_content} %>
  <%= submit_tag t(:button_save) %>
<% end %>